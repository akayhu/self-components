{
  "name": "self-components",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://akayhu.github.io/self-components/",
  "author": "Akay",
  "dependencies": {
    "antd": "^3.15.1",
    "babel-plugin-import": "^1.11.0",
    "husky": "^1.3.1",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "react": "^16.8.4",
    "react-docgen-typescript-loader": "^3.0.1",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router": "^4.4.0",
    "react-router-dom": "^4.4.0",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "sass-loader": "^7.1.0"
  },
  "devDependencies": {
    "imagemin-cli": "^3.0.0",
    "imagemin-lint-staged": "^0.3.0",
    "lint-staged": "^8.0.5",
    "@babel/core": "^7.3.4",
    "@storybook/addon-actions": "^5.0.5",
    "@storybook/addon-backgrounds": "^5.0.5",
    "@storybook/addon-centered": "^5.0.5",
    "@storybook/addon-jest": "^5.0.5",
    "@storybook/addon-knobs": "^5.0.5",
    "@storybook/addon-links": "^5.0.5",
    "@storybook/addon-notes": "^5.0.5",
    "@storybook/addon-options": "^5.0.5",
    "@storybook/addon-storyshots": "^5.0.5",
    "@storybook/addon-storysource": "^5.0.5",
    "@storybook/addon-viewport": "^5.0.5",
    "@storybook/addons": "^5.0.5",
    "@storybook/react": "^5.0.5",
    "babel-loader": "^8.0.5",
    "storybook-addon-react-docgen": "^1.2.0",
    "storybook-addon-styled-component-theme": "^1.1.1",
    "storybook-readme": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "prettier": "prettier --use-tabs --single-quote --trailing-comma es5 --write {src,public}/**/*.{js,jsx,mjs,json,md,css,sass}"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,public}/**/*.{js,jsx,json,md,css,sass}": [
      "npm run prettier",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  }
}
